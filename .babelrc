{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "chrome": 49
                },
                "include": ["transform-es2015-classes"]
            }
        ],
        "stage-0"
    ],
    "plugins": ["transform-decorators-legacy", ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }], "external-helpers"],
    "env": {
        "test": {
            "presets": [["env"], "stage-0"],
            "plugins": ["transform-decorators-legacy"]
        }
    }
}
